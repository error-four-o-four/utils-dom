{
  "name": "dom",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/dom.js",
      "require": "./lib/dom.umd.cjs"
    }
  },
  "main": "./lib/dom.umd.cjs",
  "module": "./lib/dom.js",
  "typings": "./lib/main.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
