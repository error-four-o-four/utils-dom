  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.11",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.4.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitest": "^0.25.6"
  }
